{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "RhetosConnectionString": "<YOURDBCONNECTIONSTRING>"
  },
  "Rhetos": {
    "AppSecurity": {
      "AllClaimsForAnonymous": false,
      "AllClaimsForUsers": "SampleUser@<YOURMACHINENAME>"
    }
  },
  "AllowedHosts": "*"
}
